; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Catacombs of Gore"
config/description="PS1-style action RPG based on the Catacombs of Gore tabletop RPG"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/autoload/game_manager.gd"
CombatManager="*res://scripts/autoload/combat_manager.gd"
InventoryManager="*res://scripts/autoload/inventory_manager.gd"
QuestManager="*res://scripts/autoload/quest_manager.gd"
AudioManager="*res://scripts/autoload/audio_manager.gd"
SaveManager="*res://scripts/autoload/save_manager.gd"
SceneManager="*res://scripts/autoload/scene_manager.gd"
LootTables="*res://scripts/data/loot_tables.gd"
MapTracker="*res://scripts/autoload/map_tracker.gd"
RestManager="*res://scripts/autoload/rest_manager.gd"
DiceManager="*res://scripts/autoload/dice_manager.gd"
CraftingManager="*res://scripts/autoload/crafting_manager.gd"
DialogueManager="*res://scripts/autoload/dialogue_manager.gd"
ConversationSystem="*res://scripts/autoload/conversation_system.gd"
BoatTravelManager="*res://scripts/autoload/boat_travel_manager.gd"
CrimeManager="*res://scripts/autoload/crime_manager.gd"
BountyManager="*res://scripts/autoload/bounty_manager.gd"
WorldManager="*res://scripts/autoload/world_manager.gd"
FastTravelManager="*res://scripts/autoload/fast_travel_manager.gd"
BackgroundManager="*res://scripts/autoload/background_manager.gd"
EncounterManager="*res://scripts/autoload/encounter_manager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/size/window_width_override=1280
window/size/window_height_override=960
window/stretch/mode="viewport"

